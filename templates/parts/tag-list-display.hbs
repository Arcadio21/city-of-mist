<ul class="tag-list">
	{{#each itemlist as |tag |}}
	{{#if (eq tag.type "tag")}}
	{{#if (eq tag.system.theme_id ../themeId)}}
	{{#if (eq tag.system.subtype ../subtype)}}
	{{#if (eq tag.system.parentId ../supertag)}}
	<li>
		{{> "systems/city-of-mist/templates/parts/tag-display.html" tag=tag owner=../owner locked=../locked sheetowner=../sheetowner canBurn=true data=tag.system}}
	{{#if tag.subtags}}
	{{#each tag.subtags as |st|}}
	<ul class="sub-tag-list">
		<li>
		{{> "systems/city-of-mist/templates/parts/tag-display.html" tag=st owner=../../owner locked=../../locked sheetowner=../../sheetowner canBurn=true data=st.system}}
		</li>
	</ul>
	{{/each}}
	</li>
	{{/if}}
	{{/if}}
	{{/if}}
	{{/if}}
	{{/if}}
	{{/each}}
</ul>
